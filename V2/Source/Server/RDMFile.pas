procedure TRemoteModule.Validar_Ofertas;
begin
  Try
    If Not spVALIDAR_OFERTAS.Transaction.Active Then
      spVALIDAR_OFERTAS.Transaction.StartTransaction;

    spVALIDAR_OFERTAS.Prepare;
    spVALIDAR_OFERTAS.ExecProc;

    If spVALIDAR_OFERTAS.Transaction.Active Then
      spVALIDAR_OFERTAS.Transaction.Commit;

    spVALIDAR_OFERTAS.UnPrepare;
    spVALIDAR_OFERTAS.Close;
  Except

  End;
end;

function TRemoteModule.Validar_Codigo_Barra(const Codigo_Barra: WideString;
  out Codigo, Descripcion: WideString): Shortint;
begin
  Try
    Try
      Codigo := '';
      Descripcion := '';

      If Not spVALIDAD_CODIGOS_BARRA.Transaction.Active Then
        spVALIDAD_CODIGOS_BARRA.Transaction.StartTransaction;
      spVALIDAD_CODIGOS_BARRA.ParamByName('P_CODIGO_BARRA').Value := Codigo_Barra;
      spVALIDAD_CODIGOS_BARRA.Prepare;
      spVALIDAD_CODIGOS_BARRA.ExecProc;
      Result := spVALIDAD_CODIGOS_BARRA.ParamByName('P_RESULT').Value;
      If Result = 1 Then
        Begin
          Codigo := Trim(spVALIDAD_CODIGOS_BARRA.ParamByName('P_CODE').Value);
          Descripcion := Trim(spVALIDAD_CODIGOS_BARRA.ParamByName('P_DESCRIPTION').Value);
        End;
    Except
      On E:Exception Do
        Begin
           Result := 2;
           Descripcion := E.Message;
        End;
    End;
  Finally
    spVALIDAD_CODIGOS_BARRA.UnPrepare;
    spVALIDAD_CODIGOS_BARRA.Close
  End;
end;

