procedure TReportsAndForms.ppDetailBand44BeforePrint(Sender: TObject);
  Var
    Jpg       : TGraphic;
    MemStream : TMemoryStream;
  Begin
    If PipeReportes.DataSource.DataSet.FieldByName('Foto').IsNull Then
      Begin
        Foto_Catalogo.Picture.LoadFromFile(_DefaultImgDir + _DefaultItemImg);
        Exit;
      End;

    Jpg := Nil;
    MemStream := TMemoryStream.Create;
    Try
      Jpg := TJpegImage.Create;
      Try
        (PipeReportes.DataSource.DataSet.FieldByName('Foto') As TBlobField).SaveToStream(MemStream);
        MemStream.Seek(0,0);
        Jpg.LoadFromStream(MemStream);
        Foto_Catalogo.Picture.Graphic:= jpg;
      Except
        Foto_Catalogo.Picture.LoadFromFile(_DefaultImgDir + _DefaultItemImg);
      End; {Try}
    Finally
      Jpg.Free;
      MemStream.Free;
    end {Try}
end;

